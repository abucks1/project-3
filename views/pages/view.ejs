    <body>

    <h3> Current schedules</h3>
<% for (let i = 0; i < schedules.length; i++) { %>
    <div class="flexsch">
    <p><strong></strong><%=schedules[i].username%></p>
    <p><strong></strong><%=schedules[i].day%></p>
    <p><strong></strong><%=schedules[i].start_time%></p>
    <p><strong></strong><%=schedules[i].end_time%></p>
    </div>
<% } %>

<a href="/new"><button id = "new">Add new schedules</button></a>


</body>
